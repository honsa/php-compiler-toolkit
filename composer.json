{
    "name": "ircmaxell/php-compiler-toolkit",
    "description": "An abstraction around several compiler libraries",
    "license": "MIT",
    "authors": [
        {
            "name": "Anthony Ferrara",
            "email": "ircmaxell@gmail.com"
        }
