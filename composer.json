        "php": ">=7.4",
        "ircmaxell/libgccjit": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "PHPCompilerToolkit\\": "lib/"
        },
        "files": [
            "ffi/libgccjit.php",
            "ffi/libjit.php"
        ]
    }

}
